{
    "compilerOptions": {
      "outDir": "./public",
      "target": "es5",
      "module": "commonjs",
      "noImplicitAny": false,
      "removeComments": true,
      "sourceMap": true,
      "jsx": "react",
      "jsxFactory": "h"
    },
    "include": [
      "./src/**/*.tsx",
      "./src/**/*.ts",
      "./test/**/*.tsx",
      "./test/**/*.ts",
      "node_modules/@types"
    ]
  }